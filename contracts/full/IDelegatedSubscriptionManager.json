  "source": "pragma solidity ^0.5.4;\n\nimport \"./ISubscriptionManager.sol\";\n\n/**\n * @title Subscription manager extension for delegated execution.\n */\ncontract IDelegatedSubscriptionManager is ISubscriptionManager {\n\n    function cancel(\n        bytes32 subscriptionHash,\n        bytes calldata signature\n    )\n        external;\n\n    function getActionHash(\n        string memory action,\n        bytes32 subscriptionHash\n    )\n        public\n        pure\n        returns(bytes32);\n\n    function getSigner(bytes32 hash, bytes memory signature)\n        public\n        view\n        returns(address);\n}\n",
